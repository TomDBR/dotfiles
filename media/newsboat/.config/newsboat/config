external-url-viewer "urlscan --run 'webview {}'"
browser webview
show-read-feeds no
bind-key j down
bind-key k up
bind-key j next articlelist
bind-key k prev articlelist
bind-key J next-feed articlelist
bind-key K prev-feed articlelist
bind-key G end
bind-key g home
bind-key l open
bind-key h quit
bind-key x toggle-show-read-feeds


macro y set browser "mpv --no-video --audio-display=no %u"; open-in-browser ; set browser "webview"
macro q set browser "surf -F -s -g -n -p %u"; open-in-browser ; set browser "webview"
